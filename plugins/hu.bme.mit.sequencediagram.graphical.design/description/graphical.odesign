<?xml version="1.0" encoding="UTF-8"?>
<description:Group xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:description="http://www.eclipse.org/sirius/description/1.1.0" xmlns:description_1="http://www.eclipse.org/sirius/diagram/sequence/description/2.0.0" xmlns:style="http://www.eclipse.org/sirius/diagram/description/style/1.1.0" xmlns:tool="http://www.eclipse.org/sirius/diagram/sequence/description/tool/2.0.0" xmlns:tool_1="http://www.eclipse.org/sirius/description/tool/1.1.0" name="graphical" version="12.0.0.2017041100">
  <ownedViewpoints name="InteractionModeling" label="Interaction Modeling">
    <ownedRepresentations xsi:type="description_1:SequenceDiagramDescription" name="AutomatonSequenceDiagram" label="Automaton Sequence Diagram" domainClass="Interaction" enablePopupBars="true" endsOrdering="feature:events">
      <metamodel href="http://www.eclipse.org/emf/2002/Ecore#/"/>
      <metamodel href="../../hu.bme.mit.sequencediagram.graphical.model/model/graphical.ecore#/"/>
      <defaultLayer name="Default">
        <nodeMappings xsi:type="description_1:InstanceRoleMapping" name="Component" semanticCandidatesExpression="feature:components" synchronizationLock="true" domainClass="graphical.Component">
          <borderedNodeMappings xsi:type="description_1:ExecutionMapping" name="Lifeline" semanticCandidatesExpression="var:self" semanticElements="var:self" synchronizationLock="true" domainClass="graphical.Component" startingEndFinderExpression="var:self" finishingEndFinderExpression="var:self">
            <style xsi:type="style:SquareDescription" borderSizeComputationExpression="3" showIcon="false" labelExpression="" resizeKind="NSEW" width="1" height="40">
              <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='blue']"/>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='white']"/>
            </style>
          </borderedNodeMappings>
          <style xsi:type="style:SquareDescription" labelSize="12" showIcon="false" sizeComputationExpression="" labelPosition="node" resizeKind="NSEW" width="20" height="5">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='blue']"/>
          </style>
        </nodeMappings>
        <edgeMappings xsi:type="description_1:BasicMessageMapping" name="Message" semanticCandidatesExpression="feature:messages" synchronizationLock="true" sourceMapping="//@ownedViewpoints[name='InteractionModeling']/@ownedRepresentations[name='AutomatonSequenceDiagram']/@defaultLayer/@nodeMappings[name='Component']/@borderedNodeMappings[name='Lifeline']" targetMapping="//@ownedViewpoints[name='InteractionModeling']/@ownedRepresentations[name='AutomatonSequenceDiagram']/@defaultLayer/@nodeMappings[name='Component']/@borderedNodeMappings[name='Lifeline']" targetFinderExpression="aql:self.targetEvent.context" sourceFinderExpression="aql:self.sourceEvent.context" domainClass="graphical.Message" useDomainElement="true" sendingEndFinderExpression="feature:sourceEvent" receivingEndFinderExpression="feature:targetEvent">
          <style sizeComputationExpression="2">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='red']"/>
            <centerLabelStyleDescription showIcon="false" labelExpression="aql:self.content.name">
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
        <containerMappings xsi:type="description_1:CombinedFragmentMapping" name="OptFragment" label="Opt Fragment" preconditionExpression="aql:not self.startEvent.context->isEmpty()" semanticCandidatesExpression="feature:eAllContents" semanticElements="var:self" synchronizationLock="true" domainClass="graphical.OptFragment" startingEndFinderExpression="feature:startEvent" finishingEndFinderExpression="feature:finishEvent" coveredLifelinesExpression="aql:self.startEvent.context">
          <subContainerMappings xsi:type="description_1:OperandMapping" name="Operand" semanticCandidatesExpression="feature:operands" semanticElements="var:self" synchronizationLock="true" domainClass="graphical.Operand" startingEndFinderExpression="feature:startEvent" finishingEndFinderExpression="aql:self.eContainer().finishEvent">
            <style xsi:type="style:FlatContainerStyleDescription" arcWidth="1" arcHeight="1" borderSizeComputationExpression="1" labelExpression="operand" labelAlignment="LEFT" backgroundStyle="GradientTopToBottom">
              <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='light_blue']"/>
              <backgroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
              <foregroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
            </style>
          </subContainerMappings>
          <style xsi:type="style:FlatContainerStyleDescription" arcWidth="1" arcHeight="1" borderSizeComputationExpression="1" showIcon="false" labelExpression="opt" labelAlignment="LEFT">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='light_green']"/>
            <backgroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='orange']"/>
            <foregroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='orange']"/>
            <labelBorderStyle href="environment:/viewpoint#//@labelBorderStyles/@labelBorderStyleDescriptions.0"/>
          </style>
        </containerMappings>
        <toolSections name="Components">
          <ownedTools xsi:type="tool:InstanceRoleCreationTool" name="Component" nodeMappings="//@ownedViewpoints[name='InteractionModeling']/@ownedRepresentations[name='AutomatonSequenceDiagram']/@defaultLayer/@nodeMappings[name='Component']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:container">
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="graphical.Component" referenceName="components">
                  <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:instance">
                    <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="aql:'newComponent' + instance.eContainer(graphical::Interaction).components->size()"/>
                  </subModelOperations>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
            <predecessor name="predecessor"/>
          </ownedTools>
        </toolSections>
        <toolSections name="Messages">
          <ownedTools xsi:type="tool:MessageCreationTool" name="Message" forceRefresh="true" edgeMappings="//@ownedViewpoints[name='InteractionModeling']/@ownedRepresentations[name='AutomatonSequenceDiagram']/@defaultLayer/@edgeMappings[name='Message']">
            <sourceVariable name="source"/>
            <targetVariable name="target"/>
            <sourceViewVariable name="sourceView"/>
            <targetViewVariable name="targetView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="aql:source.eContainer(graphical::Interaction)">
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="graphical.Message" referenceName="messages" variableName="message">
                  <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:message">
                    <subModelOperations xsi:type="tool_1:CreateInstance" typeName="graphical.MessageContent" referenceName="content" variableName="messageContent">
                      <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:messageContent">
                        <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="empty"/>
                      </subModelOperations>
                    </subModelOperations>
                  </subModelOperations>
                </subModelOperations>
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="graphical.MessageEvent" referenceName="events" variableName="sendingEvent">
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="context" valueExpression="var:source"/>
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="message" valueExpression="var:message"/>
                </subModelOperations>
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="graphical.MessageEvent" referenceName="events" variableName="receivingEvent">
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="context" valueExpression="var:target"/>
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="message" valueExpression="var:message"/>
                </subModelOperations>
                <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:message">
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="sourceEvent" valueExpression="var:sendingEvent"/>
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="targetEvent" valueExpression="var:receivingEvent"/>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
            <startingEndPredecessor name="startingEndPredecessor"/>
            <finishingEndPredecessor name="finishingEndPredecessor"/>
          </ownedTools>
        </toolSections>
        <toolSections name="ControlFlow" label="Control Flow">
          <ownedTools xsi:type="tool:CombinedFragmentCreationTool" name="OptFragment" containerMappings="//@ownedViewpoints[name='InteractionModeling']/@ownedRepresentations[name='AutomatonSequenceDiagram']/@defaultLayer/@containerMappings[name='OptFragment']" extraMappings="//@ownedViewpoints[name='InteractionModeling']/@ownedRepresentations[name='AutomatonSequenceDiagram']/@defaultLayer/@containerMappings[name='OptFragment']/@subContainerMappings[name='Operand']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="aql:self.eContainerOrSelf(graphical::Interaction)">
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="graphical.OptFragment" referenceName="combinedFragments" variableName="cf">
                  <subModelOperations xsi:type="tool_1:CreateInstance" typeName="graphical.Operand" referenceName="operands" variableName="op">
                    <subModelOperations xsi:type="tool_1:SetValue" featureName="expression" valueExpression="emptyOperand"/>
                  </subModelOperations>
                </subModelOperations>
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="graphical.CombinedFragmentEvent" referenceName="events" variableName="startCf">
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="context" valueExpression="var:coverage"/>
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="combinedFragment" valueExpression="var:cf"/>
                </subModelOperations>
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="graphical.CombinedFragmentEvent" referenceName="events" variableName="finishCf">
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="context" valueExpression="var:coverage"/>
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="combinedFragment" valueExpression="var:cf"/>
                </subModelOperations>
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="graphical.OperandEvent" referenceName="events" variableName="startOp">
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="context" valueExpression="var:coverage"/>
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="operand" valueExpression="var:op"/>
                </subModelOperations>
                <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:cf">
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="startEvent" valueExpression="var:startCf"/>
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="finishEvent" valueExpression="var:finishCf"/>
                </subModelOperations>
                <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:op">
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="startEvent" valueExpression="var:startOp"/>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
            <startingEndPredecessor name="startingEndPredecessor"/>
            <finishingEndPredecessor name="finishingEndPredecessor"/>
            <coveredLifelines name="coverage"/>
          </ownedTools>
        </toolSections>
      </defaultLayer>
      <backgroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='white']"/>
    </ownedRepresentations>
    <ownedJavaExtensions qualifiedClassName="hu.bme.mit.sequencediagram.graphical.design.Services"/>
  </ownedViewpoints>
</description:Group>
